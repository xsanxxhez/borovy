{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "functions": {
    "server.js": {
      "includeFiles": [
        "routes/**",
        "config/**",
        "middleware/**",
        "controllers/**",
        "uploads/**"
      ]
    }
  }
}